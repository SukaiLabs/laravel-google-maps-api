{
    "name": "cyna/googlemaps",
    "description": "A comprehensive Laravel package for Google Maps geocoding features with address history management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cyna Team",
            "email": "dev@cyna.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/http": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Cyna\\GoogleMaps\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cyna\\GoogleMaps\\GoogleMapsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
